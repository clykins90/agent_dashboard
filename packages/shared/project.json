{
  "name": "shared",
  "root": "packages/shared",
  "sourceRoot": "packages/shared/src",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc -p tsconfig.json",
        "cwd": "packages/shared"
      },
      "outputs": [
        "packages/shared/dist"
      ]
    },
    "vite:typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc -p packages/shared/tsconfig.json --noEmit"
      }
    },
    "eslint:lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": [
          "packages/shared/src/**/*.{ts,tsx,js}"
        ]
      }
    }
  }
}


